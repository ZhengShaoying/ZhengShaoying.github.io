/1 = "N0.bmp"
/2 = "N45.bmp"
/3 = "N90.bmp"
/4 = "N135.bmp"
/5 = "N180.bmp"
/6 = "N225.bmp"
/7 = "N270.bmp"
/8 = "N315.bmp"
</item>

<item mirrorR>
